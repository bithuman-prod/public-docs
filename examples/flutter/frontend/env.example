# LiveKit Configuration
LIVEKIT_SERVER_URL=wss://your-project.livekit.cloud
LIVEKIT_TOKEN=your_livekit_token_here

# Token Generation Endpoint (Optional)
TOKEN_ENDPOINT=http://localhost:3000/token

# Room Configuration
ROOM_NAME=flutter-avatar-room
PARTICIPANT_NAME=Flutter User

# Media Configuration
ENABLE_CAMERA=true
ENABLE_MICROPHONE=true
ENABLE_SPEAKER=true

# Video Settings
VIDEO_WIDTH=1280
VIDEO_HEIGHT=720
VIDEO_FPS=30
VIDEO_BITRATE=1000000

# Audio Settings
AUDIO_BITRATE=64000
AUDIO_SAMPLE_RATE=48000
AUDIO_CHANNELS=1

# Debug Settings
ENABLE_DEBUG_LOGS=true
ENABLE_STATS=true
